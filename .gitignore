.DS_Store

# Ignore custom dependency paths
/.dependencies/custom/*
!/.dependencies/custom/.gitkeep

# Ignore generated build files
/build_files/*
!/build_files/.gitkeep

# Ignore temporary files and directories
/tmp.d/*
!/tmp.d/.gitkeep

# Symlink only valid if build succeeded
# Do not include this in the repository
# It will automatically be generated
/.napci/build_files

################################
# Repository Secrets Directory #
################################

/.secrets/github_access_token

# Commit signing key
/.secrets/keys/commit_signing/*
!/.secrets/keys/commit_signing/.gitkeep

# Github push key
/.secrets/keys/github_push/*
!/.secrets/keys/github_push/.gitkeep

# Website deploy key
/.secrets/keys/site_deploy/*
!/.secrets/keys/site_deploy/.gitkeep
